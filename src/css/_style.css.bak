/*dt*/
.dl-horizontal dt {
	float: left;
	width: auto;
	overflow: hidden;
	clear: left;
	text-align: left;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.dl-horizontal dt::after {
	margin-left: 10px;
	margin-right: 10px;
	content: "=";
}
.dl-horizontal dd {
	margin-left: 10px !important;
}
.dl-horizontal dt:nth-child(4n) {
	clear: both;
}
/* http://codepen.io/stowball/pen/ewvJl */
.dl-float {
	min-height: 20px;
	overflow: hidden;
	position: relative;
	width: 100%;
	border-radius: 3px;
	padding: 9px;
	background-color: #f5f5f5;
	border: 1px solid #e3e3e3;
	-webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, .05);
	box-shadow: inset 0 1px 1px rgba(0, 0, 0, .05);

}
.dl-float:before {
	bottom: 0;
	content: '';
	left: 0;
	position: absolute;
	top: 0;
	width: 50%;
	z-index: 1;
}
.dl-float dt, .dl-float dd {
	float: left;
	width: 24%;
	text-overflow: ellipsis;
	white-space: nowrap;
	position: relative;
	z-index: 2;
}

.dl-float dt:nth-child(4n) {
	clear: both;
}
.dl-float dt::after {
	margin-left: 10px;
	margin-right: 10px;
	content: "=";
}
.dl-float dd {
	margin-left: -110px !important;
}
