/*
	 All form elements including input, select, textarea etc.
-----------------------------------------------------------------
*/
.form-control {
	-webkit-border-radius: 0px !important;
	-moz-border-radius: 0px !important;
	border-radius: 0px !important;
	box-shadow: none;
}

.form-control.line {
	border-top: none;
	border-right: none;
	border-left: none;
	border-bottom: 1px solid #367fa9;
	/* box-shadow: 2px 1px #888888; */
}

.form-control:focus {
	border-color: #3c8dbc !important;
	box-shadow: none;
}

.form-group.has-success label {
	color: #00a65a;
}

.form-group.has-success .form-control {
	border-color: #00a65a !important;
	box-shadow: none;
}

.form-group.has-warning label {
	color: #f39c12;
}

.form-group.has-warning .form-control {
	border-color: #f39c12 !important;
	box-shadow: none;
}

.form-group.has-error label {
	color: #f56954;
}

.form-group.has-error .form-control {
	border-color: #f56954 !important;
	box-shadow: none;
}
/* Fgroup */
.form-group.form-group-small {
	height: 34px;
	width: 100px !important;
	-webkit-border-radius: 0px !important;
	-moz-border-radius: 0px !important;
	border-radius: 0px !important;
	box-shadow: none;
}
/* Input group */
.input-group .input-group-addon {
	border-radius: 0;
	background-color: #f4f4f4;
}

.input-group .input-group-addon.icon{
	border-top: none;
	border-right: none;
	border-left: none;
	border-bottom: 1px solid #367fa9;
	/* border: 0px 0px 1px 0px solid #367fa9; */
	background-color: #fff !important;
}

/* Input group */
.input-group {
	padding-bottom: 10px;
}

/* Input group-addon - icon widht?
.input-group-addon {
	max-width: 36px;
	min-width: 36px;
}*/

/* Input group-addon - icon widht? */
.input-icon {
	position: relative;
	display: block;
}

.input-icon>[class*="fa-"].circular, .input-icon>[class*="glyphicon-"].circular {
border-radius: 500em !important;
box-shadow: 0 0 0 .1em rgba(0,0,0,.1) inset;
-webkit-transition: -webkit-box-shadow .3s ease;
-moz-transition: -moz-box-shadow .3s ease;
-o-transition: box-shadow .3s ease;
transition: box-shadow .3s ease;
padding: 0;
top: 5px;
bottom: 5px;
left: 7px;
width: 24px;
height: 24px;
font-size: 12px;
line-height: 24px;
}

.input-icon>input {
	padding-left: 36px;
	padding-right: 6px;
	width: 100%;
}

.input-icon.icon-right>input.input-sm {
	padding-right: 26px;
}

.input-icon>input.input-xs+[class*="fa-"], .input-icon>input.input-xs+[class*="glyphicon-"] {
	width: 24px;
	font-size: 10px;
	padding-top: 0;
	line-height: 24px;
}
.input-icon.icon-right>[class*="fa-"], .input-icon.icon-right>[class*="glyphicon-"] {
	left: auto;
	right: 0;
}
.input-icon>[class*="fa-"], .input-icon>[class*="glyphicon-"] {
	z-index: 6;
	position: absolute;
	top: 0;
	bottom: 13px !important;
	cursor: pointer;
	width: 34px;
	height: 100%;
	line-height: 28px;
	display: inline-block;
	color: #555;
	font-size: 14px;
	text-align: center;
	/* padding-top: 3px; */
	zoom: 1;
	filter: alpha(opacity=70);
	-webkit-opacity: .7;
	-moz-opacity: .7;
	opacity: .7;
}

.fileUpload {
	position: relative;
	overflow: hidden;
	margin: 10px;
}

.fileUpload input.upload {
	position: absolute;
	top: 0;
	right: 0;
	margin: 0;
	padding: 0;
	font-size: 20px;
	cursor: pointer;
	opacity: 0;
	filter: alpha(opacity = 0);
}

/* button groups */
.btn-group {
	vertical-align: inherit !important;
}
.btn-group-vertical .btn.btn-flat:first-of-type, .btn-group-vertical .btn.btn-flat:last-of-type
	{
	border-radius: 0;
}
/* Checkbox and radio inputs */
.checkbox, .radio {
	padding-left: 0;
}

textarea.form-control { resize:vertical; } /* none|horizontal|vertical|both */

