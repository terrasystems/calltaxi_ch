}
/* Remove the background from the body element */
.lockscreen>body {
	background: transparent;
}
/* We will put the dynamically generated digital clock here */
.lockscreen .headline {
	color: #fff;
	text-shadow: 1px 3px 5px rgba(0, 0, 0, 0.5);
