	from{-webkit-box-shadow: 0px 0px 0px rgba(#5a99d4, .75);-moz-box-shadow:    0px 0px 0px rgba(#5a99d4, .75);box-shadow:         0px 0px 0px rgba(#5a99d4, .75);
	}
	50%{-webkit-box-shadow: 0px 0px 35px rgba(#5a99d4, .75);-moz-box-shadow:    0px 0px 30px rgba(#5a99d4, .75);box-shadow:         0px 0px 30px rgba(#5a99d4, .75);
	}
	to{-webkit-box-shadow: 0px 0px 0px rgba(#5a99d4, .75);-moz-box-shadow:    0px 0px 0px rgba(#5a99d4, .75);box-shadow:         0px 0px 0px rgba(#5a99d4, .75);
	}
}
