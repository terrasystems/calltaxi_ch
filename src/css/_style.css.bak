ul.f16 {
	max-width: 320px;
	display: inline-block;
	vertical-align: top;
	font-size: 12px;
}

ul.f32 {
	overflow-y: auto;
	max-height: 300px;
}

.f16 .flag, .f32 .flag {
	width: auto;
	clear: right;
}

.f16 .flag {
	padding-left: 24px;
}

.f32 .flag {
	padding-left: 44px;
	display: inline-block;
	white-space: inherit;
	vertical-align: middle !important;
}

.f16 abbr, .f16 i {
	display: inline-block;
	width: 24px;
}

.f32 abbr, .f32 i {
	display: inline-block;
	width: 32px;
	line-height: 32px;
	vertical-align: bottom;
}

.f16 i {
	margin-left: -48px;
	margin-right: 24px;
	color: silver;
}

.f32 i {
	margin-left: -76px;
	/*margin-right: 44px;*/
	color: silver;
}

.flag.deprecated {
	color: silver;
}

.flag.island {
	color: navy;
}
.copyright {
	height: 32px;
	padding-top: 6px;
	font-size: 13px;
}

/* panel from Rainbow*/
.databox-container{
	text-align:center;
	font-size:0
}
.databox.fade {
	transition:0.5s linear all;
	opacity:1;
}
/* The starting CSS styles for the enter animation */
.databox.fade.ng-leave {
	transition:0.3s linear all;
	opacity:0;
}
.databox.fade.ng-leave.ng-leave-active {
	opacity:0;
}
.databox.fade.ng-enter {
	transition:0.3s linear all;
	opacity:0;
}
.databox.fade.ng-enter.ng-enter-active {
	opacity:1;
}

.databox{
	display:inline-block;
	width:100%;
	height:65px;
	padding:0;
	font-size:0;
